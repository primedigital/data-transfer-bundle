{
    "name": "primedigital/data-transfer-bundle",
    "description": "Console Commands to easily transfer live data (database + files) to another environment",
    "type": "library",
    "keywords": [
        "library",
        "command",
        "database",
        "transfer",
        "files",
        "ezpublish",
        "ezplatform"
    ],
    "homepage": "https://github.com/primedigital/data-transfer-bundle",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Hannes Giesenow",
            "email": "hgiesenow@kuborgh.de"
        },
        {
            "name": "Ante Crnogorac",
            "email": "zwer82@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
		"symfony/console": "*",
		"symfony/process": "*"
    },
    "require-dev": {
        "symfony/console": "*",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Prime\\DataTransferBundle\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prime\\DataTransferBundle\\Tests\\": "tests"
        }
    }
}
